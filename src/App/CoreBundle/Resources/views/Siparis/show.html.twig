{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
    <a href="{{ path('SiparisGoster',{'id' : object.id}) }}" class="btn edit_link btn-small" title="{{ 'action_edit'|trans({}, 'SonataAdminBundle') }}">
        <i class="icon-zoom-in"></i>
        Göster
    </a>
    <a href="{{ path('SiparisShow',{'id' : object.id}) }}" class="btn edit_link btn-small" title="{{ 'action_edit'|trans({}, 'SonataAdminBundle') }}">
        <i class="icon-download"></i>
        İndir
    </a>
{% endif %}
